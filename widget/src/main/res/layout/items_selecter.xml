<?xml version="1.0" encoding="utf-8"?>
<cn.zmy.browser.widget.BackPressAwareFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                 xmlns:tools="http://schemas.android.com/tools"
                                                 android:layout_width="match_parent"
                                                 android:layout_height="match_parent"
                                                 android:background="#50000000">
    <LinearLayout
        android:id="@+id/viewItemsContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@android:color/white"
        android:divider="@drawable/shape_item_selector_divider"
        android:orientation="vertical"
        android:paddingLeft="15dp"
        android:paddingStart="15dp"
        android:showDividers="middle">
    </LinearLayout>
</cn.zmy.browser.widget.BackPressAwareFrameLayout>
